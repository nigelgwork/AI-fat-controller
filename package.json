{
  "name": "ai-controller",
  "version": "1.5.2",
  "description": "AI Controller - Multi-agent orchestration dashboard",
  "private": true,
  "main": "dist-electron/main.js",
  "bin": {
    "ai-phat-controller": "bin/cli.js"
  },
  "scripts": {
    "start": "pnpm run build && node dist-server/server/index.js",
    "dev": "concurrently --kill-others \"vite\" \"tsx watch server/index.ts\"",
    "build": "pnpm run build:frontend && pnpm run build:server && pnpm run copy-assets",
    "build:frontend": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:electron": "tsc -p tsconfig.electron.json",
    "copy-assets": "cp -r server/db/migrations dist-server/server/db/",
    "preview": "vite preview",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev:server": "tsx watch server/index.ts",
    "electron:dev": "concurrently --kill-others \"vite\" \"tsx watch server/index.ts\" \"wait-on http://localhost:5173 && VITE_DEV_SERVER_URL=http://localhost:5173 electron dist-electron/main.js\"",
    "electron:start": "electron .",
    "electron:build": "pnpm run build && pnpm run build:electron && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "better-sqlite3": "^11.7.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "ws": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@tanstack/react-query": "^5.90.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.17.12",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "@xyflow/react": "^12.10.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.18.0",
    "jsdom": "^28.0.0",
    "lucide-react": "^0.562.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^4.0.18",
    "wait-on": "^8.0.2",
    "zustand": "^5.0.9"
  },
  "build": {
    "appId": "com.ai-phat-controller.app",
    "productName": "AI Phat Controller",
    "files": [
      "dist/**/*",
      "dist-server/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "dist-electron/main.js"
    },
    "asarUnpack": [
      "dist/**/*",
      "dist-server/**/*",
      "node_modules/better-sqlite3/**/*"
    ],
    "linux": {
      "target": ["AppImage", "deb"],
      "category": "Development"
    },
    "win": {
      "target": ["nsis", "portable"]
    },
    "mac": {
      "target": ["dmg"]
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ],
    "overrides": {
      "tar": ">=7.5.7"
    }
  }
}
